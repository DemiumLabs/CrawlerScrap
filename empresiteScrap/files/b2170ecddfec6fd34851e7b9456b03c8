					function actualizaVotaciones(votacion) {
						var params = {
							nif: "JO6OWrBs2g8GtjODBTarug"
						};
						if (votacion != undefined) {
							params["votacion"] = votacion;
						}
					
						jQuery.ajax({
							url: "/servlet/app/portal/EMP/prod/XML_VOTACION_EMPRESA/",
							type: "POST",
							data: params,
							datatype: "xml",
							success: function(data) {
								
								var $data = jQuery(data);
								
								if ($data.find("RESULTADO").text() == "1") {
									muestraVotacion($data.find("VALOR_VOTACIONES").text(), $data.find("NUM_VOTACIONES").text());
								}
								
							}
						});
					}
					
					
					function muestraVotacion(valor_votos, num_votos) {
						
						if (!isNaN(valor_votos) && !isNaN(num_votos)) {
							var calculo_votacion = valor_votos / num_votos;
							var calculo_votacion25 = Math.floor(calculo_votacion) + 0.25;
							var calculo_votacion75 = Math.floor(calculo_votacion) + 0.75;
							var votacion = 0;
							
							
							if (calculo_votacion < calculo_votacion25) {
								votacion = Math.floor(calculo_votacion);
							} else if (calculo_votacion > calculo_votacion75) {
								votacion = Math.ceil(calculo_votacion);
							} else {
								votacion = calculo_votacion;
							}
							
							
							try {
								if (votacion != parseInt(votacion, 10)) {
									votacion = votacion.toFixed(2);
								}
							}catch(err) { }
							
							
							var clase_votacion = "cr";
							if (votacion == 0) {
								clase_votacion += "0";
							} else if (votacion < 1) {
								clase_votacion += "10";
							} else if (votacion == 1) {
								clase_votacion += "20";
							} else if (votacion < 2) {
								clase_votacion += "30";
							} else if (votacion == 2) {
								clase_votacion += "40";
							} else if (votacion < 3) {
								clase_votacion += "50";
							} else if (votacion == 3) {
								clase_votacion += "60";
							} else if (votacion < 4) {
								clase_votacion += "70";
							} else if (votacion == 4) {
								clase_votacion += "80";
							} else if (votacion < 5) {
								clase_votacion += "90";
							} else {
								clase_votacion += "100";
							}
							
							jQuery("#rating .rater").closest("span").remove();
							jQuery("#rating .current-rating").addClass(clase_votacion).text( votacion );
						}
						if (num_votos != undefined) {
							var texto_voto = "votos";
							if (num_votos == 1) {
								texto_voto = "voto";
							}
							
							try{
								
								num_votos = num_votos.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".");
							}catch(err){}
							
							jQuery("#rating-count").html("(<span itemprop='ratingCount'>" + num_votos + "</span> " + texto_voto + ")");
						}
					}
					
					jQuery(document).ready(function(){
						jQuery("#rating .rater").bind("click", function(e){
							e.preventDefault();
							var votacion = jQuery(this).text();
							actualizaVotaciones( votacion );
						});
					});
				</script></div></p></div><div class="contacto_cambio"><a class="trigger-contact" href="/servlet/app/portal/EMP/prod/PANTALLA_FORMULARIO_CONTACTO/" rel="nofollow" title="Cont&aacute;ctanos">&iquest;Quiere actualizar alg&uacute;n dato? Ind&iacute;quenoslo</a></div></div></header><div class="row" id="row_2col_izq_big"><div class="col col02"><div id="profile-summary"><section id="datos-externos"><div class="tag-wrapper"><span class="tag04 active"><a class="trigger-show" href="#datos-externos1" title="Datos aportados por la Empresa&nbsp;Aguidrovert S.l.">Datos aportados por la Empresa&nbsp;												
