										<a onclick="trackEventA('button', 'click', 'SucursalesVerTelefono', null);abreColorBox();" style="cursor: pointer;" title="Ver tel&eacute;fono"><button class="gris btn-resultado" title="Ver tel&eacute;fono">Ver Tel&eacute;fono</button></a></div></div></div><div class="col col01"><div id="map_canvas_sucursales"><img alt="localizacion de la empresa Neumaticos Soledad Sl" id="map_sucursales" style="cursor:pointer;" title="Pinche para ver la localizaci&oacute;n de la empresa "><script type="text/javascript">
			    			jQuery(document).ready(function(){
			    				jQuery("#map_sucursales").attr("src", "/empresite/img/es/mapa/maps_empresite.jpg");
			    			});
			    		</script></div><div id="caja_busqueda_ruta_sucursales" style="display: none;"><input class="text01 long01" id="origen_ruta_sucursal" name="origen_ruta_sucursal" placeholder="Introduzca punto de partida de la ruta" style="margin-top:10px;" type="text"> <button class="btn btn01" id="calcula_ruta_sucursal">Calcular ruta</button></div></div></div><div class="more-wrapper"><a class="more" onclick="abreColorBox();" style="cursor: pointer;" title="Ver todas las sucursales">Ver todas las Sucurales</a></div></div><script type="text/javascript">
										
		jQuery(document).ready(function(){
			initMapaSucursales();
		});
		
		var coordenadas_sucursales; // coordenadas de todas las sucursales
		
		// inicializa el mapa de sucursales
		function initMapaSucursales() {
			var $map_sucursales = jQuery("#map_sucursales");
			if ( $map_sucursales.length ) {
			
				// EVENTO CLICK SOBRE MAPA
				$map_sucursales
				.bind("click", function(e) {					
					trackEventA('button', 'click', 'SucursalesMapa', null);
					
					if (window.google == undefined) {
						creaCachedScript();
						jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaMapaSucursales");
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					} else {
						cargaMapaSucursales();
					}					
				});
				
				// EVENTO CLICK SOBRE BOTON
				jQuery("#sucursales .referencia_ruta_destino").bind("click", function(e){				
					trackEventA('button', 'click', 'SucursalesComoLlegarVerEnMapa', null);
					
					var $this_object = jQuery(this);
					var coor_x = $this_object.attr("data-location-x");
					var coor_y = $this_object.attr("data-location-y");
					var data_titulo = $this_object.attr("data-titulo");
					var data_direccion = $this_object.attr("data-direccion");
				
					jQuery("#contenedor_ruta_sucursal_como_llegar").slideUp(function(){
						jQuery("#titulo_ruta_sucursal").hide();
						jQuery(this).empty();
					});
				
					jQuery("#origen_ruta_sucursal")
					.attr("data-location-x", coor_x)
					.attr("data-location-y", coor_y)
					.attr("data-titulo", data_titulo)
					.attr("data-direccion", data_direccion)
					.val("");
					jQuery("#caja_busqueda_ruta_sucursales").show();
					jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					
					if (window.google == undefined) {
						creaCachedScript();
						jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaMapaSucursal");
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					} else {
						cargaMapaSucursal();
					}					
				});
				
				
				// EVENTO CLICK SOBRE TODAS LAS SUCURSALES
				jQuery("#mostrar_todas_sucursales").bind("click", function(e){
					e.preventDefault();
					jQuery("#caja_busqueda_ruta_sucursales").hide();
					if (coordenadas_sucursales == undefined) {
						coordenadas_sucursales = new Array();
						//console.log("muestra pirulillo");
						mensajeBloqueante("Buscando sucursales...");
						jQuery.ajax({
							url: "/servlet/app/portal/EMP/prod/XML_SUCURSALES_EMPRESA/nombre/NEUMATICOS-SOLEDAD/",
							dataType: "xml",
							success: function(data) {
								var $data = jQuery(data);
								if ($data.find("RESULTADO").text() == "1") {
									var $coordenadas = $data.find("COORDENADA");
									for (var i=0; i<$coordenadas.length; i++) {
										var $coordenada = $coordenadas.eq(i);
										if ($coordenada.attr("COOR_X") != "" && $coordenada.attr("COOR_Y") != "") {
											coordenadas_sucursales.push({
												coor_x: $coordenada.attr("COOR_X").replace(",","."),
												coor_y: $coordenada.attr("COOR_Y").replace(",",".")
											});
										}
									}
									
									if (window.google == undefined) {
										creaCachedScript();
										jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaCoordenadasMapaSucursal");
									} else {
										cargaCoordenadasMapaSucursal();
									}
									
								}
							},
							complete : function() {
								//console.log("oculta pirulillo");
								mensajeBloqueanteClose();
							}
						});
					} else {
						cargaCoordenadasMapaSucursal();
					}
					
					jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					
				});
				
				
				// click sobre boton
				jQuery("#calcula_ruta_sucursal")
				.bind("click", function(e){				
					trackEventA('button', 'click', 'SucursalesRutaComoLlegar', null);
				
					e.preventDefault();
					if (jQuery("#origen_ruta_sucursal").val() != "") {
						cargaRutaSucursal();
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					}					
				});
				
				// evento intro sobre input
				jQuery("#origen_ruta_sucursal")
				.bind("keydown", function(e){
					if (e.which == 13) {
						e.preventDefault();
						// mostrar mapa
						jQuery("#calcula_ruta_sucursal").trigger("click");
					}
				});
				
				
			}
		}
		
		var mapa;
		// carga el mapa de sucursales
		function cargaMapaSucursales() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraSucursales();
		}
		
		// carga el mapa con la sucursal seleccionada
		function cargaMapaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraSucursal();
		}
		
		function cargaRutaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraRutaSucursal();
		}
		
		function cargaCoordenadasMapaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraCoordenadasSucursales();
		}
		
		
		function MapaSucursales() {
			
			var map,
				markers, // para almacenar todos los pirulillos
				marker, // para almacenar el ultimo pirulillo
				bounds,
				coordenadas,
				coor_sucursales,
				directionsDisplay,
				directionsService,
				geocoder,
				
				/* metodos privados */
				
				//
				_initVariables = function() {
					markers = [];
					coordenadas = new Array();
				},
				
				//
				_getCoordenada = function(){
					var $coor_sucursal = jQuery("#origen_ruta_sucursal");
					var coor_x = parseFloat($coor_sucursal.attr("data-location-x"));
					var coor_y = parseFloat($coor_sucursal.attr("data-location-y"));
					var coordenada;
					if (!isNaN(coor_x) && !isNaN(coor_y)) {
						coordenada = new google.maps.LatLng(coor_y, coor_x);
					}
					return coordenada;
				}, 
				
				//
				_getAddress = function(){
					var $coor_sucursal = jQuery("#origen_ruta_sucursal");
					var direccion = $coor_sucursal.attr("data-direccion");
					var address;
					if (direccion != "") {
						address = direccion;
					}
					return address;
				},
				
				//
				_initMarkers = function() {
					var $coor_sucursales = jQuery(".sucursales button.referencia_ruta_destino.comollegar");
					_eliminaMarkers();
					for (var i=0; i < $coor_sucursales.length; i++) {
						var $coor_sucursal = $coor_sucursales.eq(i);
						coordenadas.push(
							new google.maps.LatLng($coor_sucursal.attr("data-location-y"), $coor_sucursal.attr("data-location-x"))
						);
						_addMarker(coordenadas[i]);
					}
				},
				
				//
				_muestraMarkers = function() {
					_initMarkers();
					
					if (bounds == undefined) {
						bounds = new google.maps.LatLngBounds();
					}
					for (var i=0; i < coordenadas.length; i++) {
						bounds.extend(coordenadas[i]);						
					}
					map.fitBounds(bounds);
				},
				
				//
				_muestraMarkersSucursales = function() {
				
					// si no hay sucursales, no pintarlas
					if (coordenadas_sucursales.length) {
				
						if (bounds == undefined) {
							bounds = new google.maps.LatLngBounds();	
						}
						_eliminaMarkers();
						coor_sucursales = new Array();
						for (var i=0; i < coordenadas_sucursales.length; i++) {
							var o_coor = coordenadas_sucursales[i];
							//console.log(o_coor);
							var latlng = new google.maps.LatLng( parseFloat(o_coor.coor_y), parseFloat(o_coor.coor_x) );
							_addMarker(latlng);
							bounds.extend(latlng);
						}
						
						map.fitBounds(bounds);
					}
				},
				
				//
				_muestraMarker = function() {
					var direccion = _getAddress();
					_eliminaMarkers();
					if (marker != undefined) {
						marker.setMap(null);
					}
					if (direccion != undefined) {
						
						if (geocoder == undefined) {
							geocoder = new google.maps.Geocoder();
						}
				        geocoder.geocode( { 'address': direccion}, function(results, status) {
				            if (status == google.maps.GeocoderStatus.OK) {
				                marker = new google.maps.Marker({
				                    map: map,
				                    position: results[0].geometry.location
				                });
				                map.setCenter(results[0].geometry.location);
				        		map.setZoom(12);
				            }
				        });
			        }
				},
				
				//
				_addMarker = function( coordenada ) {
					var marker = new google.maps.Marker({
			           	map: map,
			            position: coordenada
			        });
			        markers.push(marker);
				},
				
				//
				_initMapa = function() {
					
					// anadir padding para ver el mapa
					//jQuery("#map_canvas_sucursales").css("padding-bottom", "100%");
					var mapOptions = {
						zoom: 5,
						center: new google.maps.LatLng(40.396764, -3.713379), // coordenada de la peninsula
						mapTypeId: google.maps.MapTypeId.ROADMAP
					}
					map = new google.maps.Map(document.getElementById("map_canvas_sucursales"), mapOptions);
				
				},
				
				//
				_muestraRutaSucursal = function() {
					if (map == undefined) {
						_initMapa();
					}
					if (directionsService == undefined) {
						directionsService = new google.maps.DirectionsService();
					}
					if (directionsDisplay == undefined) {
						directionsDisplay = new google.maps.DirectionsRenderer();
						directionsDisplay.setMap(map);
						directionsDisplay.setPanel(document.getElementById("contenedor_ruta_sucursal_como_llegar"));
					}
					
					// opciones de busqueda
					var coordenada = _getCoordenada();
					if (coordenada == undefined) {
						coordenada = jQuery("#origen_ruta_sucursal").attr("data-direccion");
					}
					var request = {
						origin: jQuery("#origen_ruta_sucursal").val(),
						destination: coordenada,
						travelMode: google.maps.TravelMode.DRIVING
					};
					
					// peticion de calculo de ruta
					directionsService.route(request, function(result, status) {
						if (status == google.maps.DirectionsStatus.OK) {
							directionsDisplay.setDirections(result);
						}
					});
					
					jQuery("#contenedor_ruta_sucursal_como_llegar").slideDown();
					// mostrar titulo de ruta
					var $contenedor_titulo = jQuery("#titulo_ruta_sucursal");
					$contenedor_titulo.find("#titulo_ruta").html(jQuery("#origen_ruta_sucursal").attr("data-titulo"));
					$contenedor_titulo.find("#direccion_ruta").html(jQuery("#origen_ruta_sucursal").val());
					$contenedor_titulo.show();
						
				},
				
				//
				_muestraCoordenadasSucursales = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarkersSucursales();
				},
				
				//
				_eliminaMarkers = function(){
					if (markers != undefined) {
						for (var i=0; i< markers.length; i++) {
							markers[i].setMap(null);
						}
						markers = [];
					}
				}
				;
		
				//
				this.muestraSucursales = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarkers();
				};
				
				//
				this.muestraCoordenadasSucursales = function() {
					_muestraCoordenadasSucursales();
				};
				
				//
				this.muestraSucursal = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarker();
				};
				
				//
				this.muestraRutaSucursal = function() {
					_muestraRutaSucursal();
				};
				
				
		
				_initVariables();
				
		}
		
		function creaCachedScript(){
		 	if (jQuery.cachedScript == undefined) {
				// cachedscript para cargar scripts por ajax
				jQuery.cachedScript = function( url, options ) { 
					options = jQuery.extend( options || {}, {
				    	dataType: "script",
				    	cache: true,
				    	url: url
				  	});
				  	return jQuery.ajax( options );
				};
			}
		}
		
	</script><div><h2 class="title02" style="text-transform: capitalize;">&Uacute;ltimas Noticias de Neumaticos Soledad Sl <span class="arrow"></span></h2><article class="article02"><header><div class="title03">EXPANSI&Oacute;N DE VALENCIA - INFORMACION FINANCIERA</div><ul class="meta"><li>Publicada el 18/10/2017</ul></header><div><p>La sociedad GRUPEVA, matriz del grupo SOLEDAD, termin&oacute; el ejercicio con una facturaci&oacute;n consolidada de 294 millones de euros, un 3% menos que el a&ntilde;o anterior. Paralelamente, el beneficio neto de la firma se increment&oacute; un 26%, para alcanzar los 6,8 millones. El buque insignia del grupo es NEUM&Aacute;TICOS SOLEDAD, firma que fue la responsable del resultado conjunto del grupo. Su cifra de negocio fue de 258 millones en 2016, un 2,6% menos, situ&aacute;ndose el beneficio en 2,24 millones, un 7% m&aacute;s. La compa&ntilde;&iacute;a tambi&eacute;n detalla que el a&ntilde;o pasado realiz&oacute; inversiones en inmovilizado por 4,53 millones de euros y ha extendido su red de almacenes y talleres.</p></div></article><article class="article02"><header><div class="title03">INFORMACI&Oacute;N - INFORMACION GENERAL</div><ul class="meta"><li>Publicada el 11/04/2017</ul></header><div><p>El Grupo SOLEDAD ha adquirido al menos cinco inmuebles que anteriormente fueron propiedad de la firma de calzado Kelme en Elche Parque Empresarial, en Torrellano, entre ellos su antigua sede principal, un edificio que se va a transformar ahora en un centro de negocios. En concreto, el antiguo centro neur&aacute;lgico de Kelme cuenta con 6.667 metros cuadrados, mientras que en este lote adquirido por la empresa se encuentra tambi&eacute;n una nave log&iacute;stica de 11.035 metros cuadrados, as&iacute; como otras tres naves industriales que suman en total casi 5.000 metros cuadrados.</p></div></article><article class="article02"><header><div class="title03">EL MUNDO DEL SIGLO VEINTIUNO - INFORMACION GENERAL</div><ul class="meta"><li>Publicada el 20/02/2017</ul></header><div><p>SOLVIA, la inmobiliaria del Banco Sabadell, sigue con su particular proceso de venta de activos empresariales no estrat&eacute;gicos. Esta vez el turno le ha tocado a dos naves industriales y un edificio de oficinas. Se trata de las antiguas instalaciones que ten&iacute;a la firma de calzado deportivo Kelme en el Elche Parque Empresarial. La compa&ntilde;&iacute;a inmobiliaria ha vendido los inmuebles al Grupo SOLEDAD, la empresa de la familia P&eacute;rez especializada en el sector del neum&aacute;tico. El precio de la operaci&oacute;n ronda los 6,5 millones de euros. Las naves Las adquiridas, con una superficie de unos 18.000 metros cuadrados, eran propiedad del Sabadell, de BBVA y del Popular.</p></div></article><article class="article02"><header><div class="title03">EXPANSI&Oacute;N DE VALENCIA - INFORMACION FINANCIERA</div><ul class="meta"><li>Publicada el 11/11/2016</ul></header><div><p>El Grupo SOLEDAD, cuyo buque insignia es NEUM&Aacute;TICOS SOLEDAD, ha logrado rebasar la barrera de los trescientos millones de euros de negocio.
Seg&uacute;n muestra la memoria de la matriz, GRUPEVA, que agrupa sus compa&ntilde;&iacute;as de la industria del neum&aacute;tico, que va desde la producci&oacute;n, distribuci&oacute;n y comercializaci&oacute;n, hasta el reciclaje y tratamiento del caucho. En 2015, el grupo tuvo una cifra de negocios de 304 millones de euros, un 4,6% m&aacute;s que un a&ntilde;o antes, y el beneficio fue de 5,4 millones. La mayor empresa del grupo, NEUM&Aacute;TICOS SOLEDAD, represent&oacute; unas ventas de 255,8 millones, un 3,7% m&aacute;s. Cuenta con varias filiales dedicadas a talleres y dos filiales internacionales, en Francia y Portugal.</p></div></article><article class="article02"><header><div class="title03">CINCO D&Iacute;AS - ACUERDO ENTRE SOCIEDADES</div><ul class="meta"><li>Publicada el 20/06/2016</ul></header><div><p>La red de talleres de ECOLOGICAL DRIVE ha dado al reciclado de neum&aacute;ticos una nueva utilidad, la reforestaci&oacute;n de zonas arboladas. Para ello, ha firmado un acuerdo con REFORESTA, asociaci&oacute;n que ha empleado fondos, en torno a 12.000 euros, procedentes de la venta de neum&aacute;ticos renovados, para la plantaci&oacute;n de especies aut&oacute;ctonas en La Pedriza y en el valle de la Barranca, ambos en Madrid, as&iacute; como en una zona de la sierra de Sevilla. ECOLOGICAL DRIVE, por su parte, calcula haber ahorrado la emisi&oacute;n de m&aacute;s de 230.000 kilos de CO2 al medio ambiente y ha dejado de consumir 580 barriles de Brent, con su apuesta por la renovaci&oacute;n de neum&aacute;ticos.</p></div></article></div><br><div class="row mobile-hidden"><div class="col col03"><section><p class="title02">categor&iacute;as</p><ul class="list02 list02b"><li class="first-child"><a href="http://empresite.eleconomista.es/Actividad/VENTA/" title="Ir a venta">venta</a><li><a href="http://empresite.eleconomista.es/Actividad/TALLER/" title="Ir a taller">taller</a><li><a href="http://empresite.eleconomista.es/Actividad/MEDIO-AMBIENTE/" title="Ir a medio ambiente">medio ambiente</a><li><a href="http://empresite.eleconomista.es/Actividad/VEHICULOS/" title="Ir a vehiculos">vehiculos</a><li><a href="http://empresite.eleconomista.es/Actividad/DISTRIBUCION/" title="Ir a Distribuci&oacute;n">Distribuci&oacute;n</a><li><a href="http://empresite.eleconomista.es/Actividad/COCHE/" title="Ir a coche">coche</a><li><a href="http://empresite.eleconomista.es/Actividad/AUTOMOVIL/" title="Ir a automovil">automovil</a><li><a href="http://empresite.eleconomista.es/Actividad/VEHICULO/" title="Ir a vehiculo">vehiculo</a><li><a href="http://empresite.eleconomista.es/Actividad/REPARACION/" title="Ir a reparaci&oacute;n">reparaci&oacute;n</a><li><a href="http://empresite.eleconomista.es/Actividad/CAMION/" title="Ir a camion">camion</a><li><a href="http://empresite.eleconomista.es/Actividad/TURISMO/" title="Ir a turismo">turismo</a><li><a href="http://empresite.eleconomista.es/Actividad/PRECIO/" title="Ir a Precio">Precio</a><li><a href="http://empresite.eleconomista.es/Actividad/CAUCHO/" title="Ir a caucho">caucho</a><li><a href="http://empresite.eleconomista.es/Actividad/MANTENIMIENTO/" title="Ir a mantenimiento">mantenimiento</a><li><a href="http://empresite.eleconomista.es/Actividad/TALLERES/" title="Ir a talleres">talleres</a><li><a href="http://empresite.eleconomista.es/Actividad/RENDIMIENTO/" title="Ir a rendimiento">rendimiento</a><li><a href="http://empresite.eleconomista.es/Actividad/RUEDAS/" title="Ir a RUEDAS">RUEDAS</a><li><a href="http://empresite.eleconomista.es/Actividad/ECOLOGIA/" title="Ir a Ecolog&iacute;a">Ecolog&iacute;a</a><li><a href="http://empresite.eleconomista.es/Actividad/MECANICA/" title="Ir a mecanica">mecanica</a><li><a href="http://empresite.eleconomista.es/Actividad/NEUMATICOS/" title="Ir a neumaticos">neumaticos</a><li><a href="http://empresite.eleconomista.es/Actividad/TODOTERRENO/" title="Ir a todoterreno">todoterreno</a><li><a href="http://empresite.eleconomista.es/Actividad/AHORRO/" title="Ir a ahorro">ahorro</a><li><a href="http://empresite.eleconomista.es/Actividad/NEUMATICO/" title="Ir a neum&aacute;tico">neum&aacute;tico</a><li><a href="http://empresite.eleconomista.es/Actividad/VENTA-NEUMATICOS/" title="Ir a venta de neum&aacute;ticos">venta de neum&aacute;ticos</a><li><a href="http://empresite.eleconomista.es/Actividad/MECANICOS/" title="Ir a mec&aacute;nicos">mec&aacute;nicos</a><li><a href="http://empresite.eleconomista.es/Actividad/RECICLAR/" title="Ir a reciclar">reciclar</a><li><a href="http://empresite.eleconomista.es/Actividad/RENOVAR/" title="Ir a renovar">renovar</a><li><a href="http://empresite.eleconomista.es/Actividad/REVISION/" title="Ir a revision">revision</a><li><a href="http://empresite.eleconomista.es/Actividad/RESPONSABLE/" title="Ir a responsable">responsable</a><li><a href="http://empresite.eleconomista.es/Actividad/REUTILIZAR/" title="Ir a reutilizar">reutilizar</a><li><a href="http://empresite.eleconomista.es/Actividad/RAPARACION/" title="Ir a raparacion">raparacion</a><li class="last-child"><a href="http://empresite.eleconomista.es/Actividad/LAVADO-ENGRASE-VEHICULOS/" title="Ir a Lavado y engrase de veh&iacute;culos">Lavado y engrase de veh&iacute;culos</a></ul></section></div></div><br><div class="solodesktop" id="banner-certificado-gestion" style="text-align:center;"></div><script type="text/javascript">
