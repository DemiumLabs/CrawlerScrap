										<a onclick="trackEventA('button', 'click', 'SucursalesVerTelefono', null);abreColorBox();" style="cursor: pointer;" title="Ver tel&eacute;fono"><button class="gris btn-resultado" title="Ver tel&eacute;fono">Ver Tel&eacute;fono</button></a></div></div></div><div class="col col01"><div id="map_canvas_sucursales"><img alt="localizacion de la empresa Hifer Homecontrol Sl" id="map_sucursales" style="cursor:pointer;" title="Pinche para ver la localizaci&oacute;n de la empresa "><script type="text/javascript">
			    			jQuery(document).ready(function(){
			    				jQuery("#map_sucursales").attr("src", "/empresite/img/es/mapa/maps_empresite.jpg");
			    			});
			    		</script></div><div id="caja_busqueda_ruta_sucursales" style="display: none;"><input class="text01 long01" id="origen_ruta_sucursal" name="origen_ruta_sucursal" placeholder="Introduzca punto de partida de la ruta" style="margin-top:10px;" type="text"> <button class="btn btn01" id="calcula_ruta_sucursal">Calcular ruta</button></div></div></div><div class="more-wrapper"><a class="more" onclick="abreColorBox();" style="cursor: pointer;" title="Ver todas las sucursales">Ver todas las Sucurales</a></div></div><script type="text/javascript">
										
		jQuery(document).ready(function(){
			initMapaSucursales();
		});
		
		var coordenadas_sucursales; // coordenadas de todas las sucursales
		
		// inicializa el mapa de sucursales
		function initMapaSucursales() {
			var $map_sucursales = jQuery("#map_sucursales");
			if ( $map_sucursales.length ) {
			
				// EVENTO CLICK SOBRE MAPA
				$map_sucursales
				.bind("click", function(e) {					
					trackEventA('button', 'click', 'SucursalesMapa', null);
					
					if (window.google == undefined) {
						creaCachedScript();
						jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaMapaSucursales");
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					} else {
						cargaMapaSucursales();
					}					
				});
				
				// EVENTO CLICK SOBRE BOTON
				jQuery("#sucursales .referencia_ruta_destino").bind("click", function(e){				
					trackEventA('button', 'click', 'SucursalesComoLlegarVerEnMapa', null);
					
					var $this_object = jQuery(this);
					var coor_x = $this_object.attr("data-location-x");
					var coor_y = $this_object.attr("data-location-y");
					var data_titulo = $this_object.attr("data-titulo");
					var data_direccion = $this_object.attr("data-direccion");
				
					jQuery("#contenedor_ruta_sucursal_como_llegar").slideUp(function(){
						jQuery("#titulo_ruta_sucursal").hide();
						jQuery(this).empty();
					});
				
					jQuery("#origen_ruta_sucursal")
					.attr("data-location-x", coor_x)
					.attr("data-location-y", coor_y)
					.attr("data-titulo", data_titulo)
					.attr("data-direccion", data_direccion)
					.val("");
					jQuery("#caja_busqueda_ruta_sucursales").show();
					jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					
					if (window.google == undefined) {
						creaCachedScript();
						jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaMapaSucursal");
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					} else {
						cargaMapaSucursal();
					}					
				});
				
				
				// EVENTO CLICK SOBRE TODAS LAS SUCURSALES
				jQuery("#mostrar_todas_sucursales").bind("click", function(e){
					e.preventDefault();
					jQuery("#caja_busqueda_ruta_sucursales").hide();
					if (coordenadas_sucursales == undefined) {
						coordenadas_sucursales = new Array();
						//console.log("muestra pirulillo");
						mensajeBloqueante("Buscando sucursales...");
						jQuery.ajax({
							url: "/servlet/app/portal/EMP/prod/XML_SUCURSALES_EMPRESA/nombre/HIFER-HOMECONTROL/",
							dataType: "xml",
							success: function(data) {
								var $data = jQuery(data);
								if ($data.find("RESULTADO").text() == "1") {
									var $coordenadas = $data.find("COORDENADA");
									for (var i=0; i<$coordenadas.length; i++) {
										var $coordenada = $coordenadas.eq(i);
										if ($coordenada.attr("COOR_X") != "" && $coordenada.attr("COOR_Y") != "") {
											coordenadas_sucursales.push({
												coor_x: $coordenada.attr("COOR_X").replace(",","."),
												coor_y: $coordenada.attr("COOR_Y").replace(",",".")
											});
										}
									}
									
									if (window.google == undefined) {
										creaCachedScript();
										jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaCoordenadasMapaSucursal");
									} else {
										cargaCoordenadasMapaSucursal();
									}
									
								}
							},
							complete : function() {
								//console.log("oculta pirulillo");
								mensajeBloqueanteClose();
							}
						});
					} else {
						cargaCoordenadasMapaSucursal();
					}
					
					jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					
				});
				
				
				// click sobre boton
				jQuery("#calcula_ruta_sucursal")
				.bind("click", function(e){				
					trackEventA('button', 'click', 'SucursalesRutaComoLlegar', null);
				
					e.preventDefault();
					if (jQuery("#origen_ruta_sucursal").val() != "") {
						cargaRutaSucursal();
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					}					
				});
				
				// evento intro sobre input
				jQuery("#origen_ruta_sucursal")
				.bind("keydown", function(e){
					if (e.which == 13) {
						e.preventDefault();
						// mostrar mapa
						jQuery("#calcula_ruta_sucursal").trigger("click");
					}
				});
				
				
			}
		}
		
		var mapa;
		// carga el mapa de sucursales
		function cargaMapaSucursales() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraSucursales();
		}
		
		// carga el mapa con la sucursal seleccionada
		function cargaMapaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraSucursal();
		}
		
		function cargaRutaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraRutaSucursal();
		}
		
		function cargaCoordenadasMapaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraCoordenadasSucursales();
		}
		
		
		function MapaSucursales() {
			
			var map,
				markers, // para almacenar todos los pirulillos
				marker, // para almacenar el ultimo pirulillo
				bounds,
				coordenadas,
				coor_sucursales,
				directionsDisplay,
				directionsService,
				geocoder,
				
				/* metodos privados */
				
				//
				_initVariables = function() {
					markers = [];
					coordenadas = new Array();
				},
				
				//
				_getCoordenada = function(){
					var $coor_sucursal = jQuery("#origen_ruta_sucursal");
					var coor_x = parseFloat($coor_sucursal.attr("data-location-x"));
					var coor_y = parseFloat($coor_sucursal.attr("data-location-y"));
					var coordenada;
					if (!isNaN(coor_x) && !isNaN(coor_y)) {
						coordenada = new google.maps.LatLng(coor_y, coor_x);
					}
					return coordenada;
				}, 
				
				//
				_getAddress = function(){
					var $coor_sucursal = jQuery("#origen_ruta_sucursal");
					var direccion = $coor_sucursal.attr("data-direccion");
					var address;
					if (direccion != "") {
						address = direccion;
					}
					return address;
				},
				
				//
				_initMarkers = function() {
					var $coor_sucursales = jQuery(".sucursales button.referencia_ruta_destino.comollegar");
					_eliminaMarkers();
					for (var i=0; i < $coor_sucursales.length; i++) {
						var $coor_sucursal = $coor_sucursales.eq(i);
						coordenadas.push(
							new google.maps.LatLng($coor_sucursal.attr("data-location-y"), $coor_sucursal.attr("data-location-x"))
						);
						_addMarker(coordenadas[i]);
					}
				},
				
				//
				_muestraMarkers = function() {
					_initMarkers();
					
					if (bounds == undefined) {
						bounds = new google.maps.LatLngBounds();
					}
					for (var i=0; i < coordenadas.length; i++) {
						bounds.extend(coordenadas[i]);						
					}
					map.fitBounds(bounds);
				},
				
				//
				_muestraMarkersSucursales = function() {
				
					// si no hay sucursales, no pintarlas
					if (coordenadas_sucursales.length) {
				
						if (bounds == undefined) {
							bounds = new google.maps.LatLngBounds();	
						}
						_eliminaMarkers();
						coor_sucursales = new Array();
						for (var i=0; i < coordenadas_sucursales.length; i++) {
							var o_coor = coordenadas_sucursales[i];
							//console.log(o_coor);
							var latlng = new google.maps.LatLng( parseFloat(o_coor.coor_y), parseFloat(o_coor.coor_x) );
							_addMarker(latlng);
							bounds.extend(latlng);
						}
						
						map.fitBounds(bounds);
					}
				},
				
				//
				_muestraMarker = function() {
					var direccion = _getAddress();
					_eliminaMarkers();
					if (marker != undefined) {
						marker.setMap(null);
					}
					if (direccion != undefined) {
						
						if (geocoder == undefined) {
							geocoder = new google.maps.Geocoder();
						}
				        geocoder.geocode( { 'address': direccion}, function(results, status) {
				            if (status == google.maps.GeocoderStatus.OK) {
				                marker = new google.maps.Marker({
				                    map: map,
				                    position: results[0].geometry.location
				                });
				                map.setCenter(results[0].geometry.location);
				        		map.setZoom(12);
				            }
				        });
			        }
				},
				
				//
				_addMarker = function( coordenada ) {
					var marker = new google.maps.Marker({
			           	map: map,
			            position: coordenada
			        });
			        markers.push(marker);
				},
				
				//
				_initMapa = function() {
					
					// anadir padding para ver el mapa
					//jQuery("#map_canvas_sucursales").css("padding-bottom", "100%");
					var mapOptions = {
						zoom: 5,
						center: new google.maps.LatLng(40.396764, -3.713379), // coordenada de la peninsula
						mapTypeId: google.maps.MapTypeId.ROADMAP
					}
					map = new google.maps.Map(document.getElementById("map_canvas_sucursales"), mapOptions);
				
				},
				
				//
				_muestraRutaSucursal = function() {
					if (map == undefined) {
						_initMapa();
					}
					if (directionsService == undefined) {
						directionsService = new google.maps.DirectionsService();
					}
					if (directionsDisplay == undefined) {
						directionsDisplay = new google.maps.DirectionsRenderer();
						directionsDisplay.setMap(map);
						directionsDisplay.setPanel(document.getElementById("contenedor_ruta_sucursal_como_llegar"));
					}
					
					// opciones de busqueda
					var coordenada = _getCoordenada();
					if (coordenada == undefined) {
						coordenada = jQuery("#origen_ruta_sucursal").attr("data-direccion");
					}
					var request = {
						origin: jQuery("#origen_ruta_sucursal").val(),
						destination: coordenada,
						travelMode: google.maps.TravelMode.DRIVING
					};
					
					// peticion de calculo de ruta
					directionsService.route(request, function(result, status) {
						if (status == google.maps.DirectionsStatus.OK) {
							directionsDisplay.setDirections(result);
						}
					});
					
					jQuery("#contenedor_ruta_sucursal_como_llegar").slideDown();
					// mostrar titulo de ruta
					var $contenedor_titulo = jQuery("#titulo_ruta_sucursal");
					$contenedor_titulo.find("#titulo_ruta").html(jQuery("#origen_ruta_sucursal").attr("data-titulo"));
					$contenedor_titulo.find("#direccion_ruta").html(jQuery("#origen_ruta_sucursal").val());
					$contenedor_titulo.show();
						
				},
				
				//
				_muestraCoordenadasSucursales = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarkersSucursales();
				},
				
				//
				_eliminaMarkers = function(){
					if (markers != undefined) {
						for (var i=0; i< markers.length; i++) {
							markers[i].setMap(null);
						}
						markers = [];
					}
				}
				;
		
				//
				this.muestraSucursales = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarkers();
				};
				
				//
				this.muestraCoordenadasSucursales = function() {
					_muestraCoordenadasSucursales();
				};
				
				//
				this.muestraSucursal = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarker();
				};
				
				//
				this.muestraRutaSucursal = function() {
					_muestraRutaSucursal();
				};
				
				
		
				_initVariables();
				
		}
		
		function creaCachedScript(){
		 	if (jQuery.cachedScript == undefined) {
				// cachedscript para cargar scripts por ajax
				jQuery.cachedScript = function( url, options ) { 
					options = jQuery.extend( options || {}, {
				    	dataType: "script",
				    	cache: true,
				    	url: url
				  	});
				  	return jQuery.ajax( options );
				};
			}
		}
		
