			    			jQuery(document).ready(function(){
			    				jQuery("#map_sucursales").attr("src", "/empresite/img/es/mapa/maps_empresite.jpg");
			    			});
			    		</script></div><div id="caja_busqueda_ruta_sucursales" style="display: none;"><input class="text01 long01" id="origen_ruta_sucursal" name="origen_ruta_sucursal" placeholder="Introduzca punto de partida de la ruta" style="margin-top:10px;" type="text"> <button class="btn btn01" id="calcula_ruta_sucursal">Calcular ruta</button></div></div></div><div class="more-wrapper"><a class="more" onclick="abreColorBox();" style="cursor: pointer;" title="Ver todas las sucursales">Ver todas las Sucurales</a></div></div><script type="text/javascript">
										
		jQuery(document).ready(function(){
			initMapaSucursales();
		});
		
		var coordenadas_sucursales; // coordenadas de todas las sucursales
		
		// inicializa el mapa de sucursales
		function initMapaSucursales() {
			var $map_sucursales = jQuery("#map_sucursales");
			if ( $map_sucursales.length ) {
			
				// EVENTO CLICK SOBRE MAPA
				$map_sucursales
				.bind("click", function(e) {					
					trackEventA('button', 'click', 'SucursalesMapa', null);
					
					if (window.google == undefined) {
						creaCachedScript();
						jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaMapaSucursales");
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					} else {
						cargaMapaSucursales();
					}					
				});
				
				// EVENTO CLICK SOBRE BOTON
				jQuery("#sucursales .referencia_ruta_destino").bind("click", function(e){				
					trackEventA('button', 'click', 'SucursalesComoLlegarVerEnMapa', null);
					
					var $this_object = jQuery(this);
					var coor_x = $this_object.attr("data-location-x");
					var coor_y = $this_object.attr("data-location-y");
					var data_titulo = $this_object.attr("data-titulo");
					var data_direccion = $this_object.attr("data-direccion");
				
					jQuery("#contenedor_ruta_sucursal_como_llegar").slideUp(function(){
						jQuery("#titulo_ruta_sucursal").hide();
						jQuery(this).empty();
					});
				
					jQuery("#origen_ruta_sucursal")
					.attr("data-location-x", coor_x)
					.attr("data-location-y", coor_y)
					.attr("data-titulo", data_titulo)
					.attr("data-direccion", data_direccion)
					.val("");
					jQuery("#caja_busqueda_ruta_sucursales").show();
					jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					
					if (window.google == undefined) {
						creaCachedScript();
						jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaMapaSucursal");
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					} else {
						cargaMapaSucursal();
					}					
				});
				
				
				// EVENTO CLICK SOBRE TODAS LAS SUCURSALES
				jQuery("#mostrar_todas_sucursales").bind("click", function(e){
					e.preventDefault();
					jQuery("#caja_busqueda_ruta_sucursales").hide();
					if (coordenadas_sucursales == undefined) {
						coordenadas_sucursales = new Array();
						//console.log("muestra pirulillo");
						mensajeBloqueante("Buscando sucursales...");
						jQuery.ajax({
							url: "/servlet/app/portal/EMP/prod/XML_SUCURSALES_EMPRESA/nombre/MERINNE-INVERSIONES-2008/",
							dataType: "xml",
							success: function(data) {
								var $data = jQuery(data);
								if ($data.find("RESULTADO").text() == "1") {
									var $coordenadas = $data.find("COORDENADA");
									for (var i=0; i<$coordenadas.length; i++) {
										var $coordenada = $coordenadas.eq(i);
										if ($coordenada.attr("COOR_X") != "" && $coordenada.attr("COOR_Y") != "") {
											coordenadas_sucursales.push({
												coor_x: $coordenada.attr("COOR_X").replace(",","."),
												coor_y: $coordenada.attr("COOR_Y").replace(",",".")
											});
										}
									}
									
									if (window.google == undefined) {
										creaCachedScript();
										jQuery.cachedScript("https://maps.google.com/maps/api/js?key=AIzaSyA10hDjGsYrWl4p8rGynce0epPCZ-JAXS4&sensor=false&callback=cargaCoordenadasMapaSucursal");
									} else {
										cargaCoordenadasMapaSucursal();
									}
									
								}
							},
							complete : function() {
								//console.log("oculta pirulillo");
								mensajeBloqueanteClose();
							}
						});
					} else {
						cargaCoordenadasMapaSucursal();
					}
					
					jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					
				});
				
				
				// click sobre boton
				jQuery("#calcula_ruta_sucursal")
				.bind("click", function(e){				
					trackEventA('button', 'click', 'SucursalesRutaComoLlegar', null);
				
					e.preventDefault();
					if (jQuery("#origen_ruta_sucursal").val() != "") {
						cargaRutaSucursal();
						jQuery("html, body").animate({ scrollTop: jQuery('#sucursales').offset().top }, 1000);
					}					
				});
				
				// evento intro sobre input
				jQuery("#origen_ruta_sucursal")
				.bind("keydown", function(e){
					if (e.which == 13) {
						e.preventDefault();
						// mostrar mapa
						jQuery("#calcula_ruta_sucursal").trigger("click");
					}
				});
				
				
			}
		}
		
		var mapa;
		// carga el mapa de sucursales
		function cargaMapaSucursales() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraSucursales();
		}
		
		// carga el mapa con la sucursal seleccionada
		function cargaMapaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraSucursal();
		}
		
		function cargaRutaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraRutaSucursal();
		}
		
		function cargaCoordenadasMapaSucursal() {
			if (mapa == undefined) {
				mapa = new MapaSucursales();
			}
			mapa.muestraCoordenadasSucursales();
		}
		
		
		function MapaSucursales() {
			
			var map,
				markers, // para almacenar todos los pirulillos
				marker, // para almacenar el ultimo pirulillo
				bounds,
				coordenadas,
				coor_sucursales,
				directionsDisplay,
				directionsService,
				geocoder,
				
				/* metodos privados */
				
				//
				_initVariables = function() {
					markers = [];
					coordenadas = new Array();
				},
				
				//
				_getCoordenada = function(){
					var $coor_sucursal = jQuery("#origen_ruta_sucursal");
					var coor_x = parseFloat($coor_sucursal.attr("data-location-x"));
					var coor_y = parseFloat($coor_sucursal.attr("data-location-y"));
					var coordenada;
					if (!isNaN(coor_x) && !isNaN(coor_y)) {
						coordenada = new google.maps.LatLng(coor_y, coor_x);
					}
					return coordenada;
				}, 
				
				//
				_getAddress = function(){
					var $coor_sucursal = jQuery("#origen_ruta_sucursal");
					var direccion = $coor_sucursal.attr("data-direccion");
					var address;
					if (direccion != "") {
						address = direccion;
					}
					return address;
				},
				
				//
				_initMarkers = function() {
					var $coor_sucursales = jQuery(".sucursales button.referencia_ruta_destino.comollegar");
					_eliminaMarkers();
					for (var i=0; i < $coor_sucursales.length; i++) {
						var $coor_sucursal = $coor_sucursales.eq(i);
						coordenadas.push(
							new google.maps.LatLng($coor_sucursal.attr("data-location-y"), $coor_sucursal.attr("data-location-x"))
						);
						_addMarker(coordenadas[i]);
					}
				},
				
				//
				_muestraMarkers = function() {
					_initMarkers();
					
					if (bounds == undefined) {
						bounds = new google.maps.LatLngBounds();
					}
					for (var i=0; i < coordenadas.length; i++) {
						bounds.extend(coordenadas[i]);						
					}
					map.fitBounds(bounds);
				},
				
				//
				_muestraMarkersSucursales = function() {
				
					// si no hay sucursales, no pintarlas
					if (coordenadas_sucursales.length) {
				
						if (bounds == undefined) {
							bounds = new google.maps.LatLngBounds();	
						}
						_eliminaMarkers();
						coor_sucursales = new Array();
						for (var i=0; i < coordenadas_sucursales.length; i++) {
							var o_coor = coordenadas_sucursales[i];
							//console.log(o_coor);
							var latlng = new google.maps.LatLng( parseFloat(o_coor.coor_y), parseFloat(o_coor.coor_x) );
							_addMarker(latlng);
							bounds.extend(latlng);
						}
						
						map.fitBounds(bounds);
					}
				},
				
				//
				_muestraMarker = function() {
					var direccion = _getAddress();
					_eliminaMarkers();
					if (marker != undefined) {
						marker.setMap(null);
					}
					if (direccion != undefined) {
						
						if (geocoder == undefined) {
							geocoder = new google.maps.Geocoder();
						}
				        geocoder.geocode( { 'address': direccion}, function(results, status) {
				            if (status == google.maps.GeocoderStatus.OK) {
				                marker = new google.maps.Marker({
				                    map: map,
				                    position: results[0].geometry.location
				                });
				                map.setCenter(results[0].geometry.location);
				        		map.setZoom(12);
				            }
				        });
			        }
				},
				
				//
				_addMarker = function( coordenada ) {
					var marker = new google.maps.Marker({
			           	map: map,
			            position: coordenada
			        });
			        markers.push(marker);
				},
				
				//
				_initMapa = function() {
					
					// anadir padding para ver el mapa
					//jQuery("#map_canvas_sucursales").css("padding-bottom", "100%");
					var mapOptions = {
						zoom: 5,
						center: new google.maps.LatLng(40.396764, -3.713379), // coordenada de la peninsula
						mapTypeId: google.maps.MapTypeId.ROADMAP
					}
					map = new google.maps.Map(document.getElementById("map_canvas_sucursales"), mapOptions);
				
				},
				
				//
				_muestraRutaSucursal = function() {
					if (map == undefined) {
						_initMapa();
					}
					if (directionsService == undefined) {
						directionsService = new google.maps.DirectionsService();
					}
					if (directionsDisplay == undefined) {
						directionsDisplay = new google.maps.DirectionsRenderer();
						directionsDisplay.setMap(map);
						directionsDisplay.setPanel(document.getElementById("contenedor_ruta_sucursal_como_llegar"));
					}
					
					// opciones de busqueda
					var coordenada = _getCoordenada();
					if (coordenada == undefined) {
						coordenada = jQuery("#origen_ruta_sucursal").attr("data-direccion");
					}
					var request = {
						origin: jQuery("#origen_ruta_sucursal").val(),
						destination: coordenada,
						travelMode: google.maps.TravelMode.DRIVING
					};
					
					// peticion de calculo de ruta
					directionsService.route(request, function(result, status) {
						if (status == google.maps.DirectionsStatus.OK) {
							directionsDisplay.setDirections(result);
						}
					});
					
					jQuery("#contenedor_ruta_sucursal_como_llegar").slideDown();
					// mostrar titulo de ruta
					var $contenedor_titulo = jQuery("#titulo_ruta_sucursal");
					$contenedor_titulo.find("#titulo_ruta").html(jQuery("#origen_ruta_sucursal").attr("data-titulo"));
					$contenedor_titulo.find("#direccion_ruta").html(jQuery("#origen_ruta_sucursal").val());
					$contenedor_titulo.show();
						
				},
				
				//
				_muestraCoordenadasSucursales = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarkersSucursales();
				},
				
				//
				_eliminaMarkers = function(){
					if (markers != undefined) {
						for (var i=0; i< markers.length; i++) {
							markers[i].setMap(null);
						}
						markers = [];
					}
				}
				;
		
				//
				this.muestraSucursales = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarkers();
				};
				
				//
				this.muestraCoordenadasSucursales = function() {
					_muestraCoordenadasSucursales();
				};
				
				//
				this.muestraSucursal = function() {
					if (map == undefined) {
						_initMapa();
					}
					_muestraMarker();
				};
				
				//
				this.muestraRutaSucursal = function() {
					_muestraRutaSucursal();
				};
				
				
		
				_initVariables();
				
		}
		
		function creaCachedScript(){
		 	if (jQuery.cachedScript == undefined) {
				// cachedscript para cargar scripts por ajax
				jQuery.cachedScript = function( url, options ) { 
					options = jQuery.extend( options || {}, {
				    	dataType: "script",
				    	cache: true,
				    	url: url
				  	});
				  	return jQuery.ajax( options );
				};
			}
		}
		
	</script><div><h2 class="title02" style="text-transform: capitalize;">&Uacute;ltimas Noticias de Noa Brands Europe Sl. <span class="arrow"></span></h2><article class="article02"><header><div class="title03">EXPANSI&Oacute;N DE CATALUNA - INFORMACION FINANCIERA</div><ul class="meta"><li>Publicada el 09/07/2018</ul></header><div><p>Desde que en 2014 la empresa catalana Noa Visual Group adquiri&oacute; la estadounidense Visual Merchandising y cre&oacute; el grupo THE VISUALITY CORPORATION (TVC), la facturaci&oacute;n ha crecido m&aacute;s de un 50%. En 2017, las ventas del grupo aumentaron un 12% y superaron la barrera de los cien millones, hasta sumar 105 millones de euros.  
La intenci&oacute;n de la compa&ntilde;&iacute;a es seguir creciendo, tanto de forma org&aacute;nica, invirtiendo en innovaci&oacute;n para seguir fabricando los maniqu&iacute;es de las grandes marcas, como a trav&eacute;s de la adquisici&oacute;n de nuevas compa&ntilde;&iacute;as, principalmente firmas tecnol&oacute;gicas para seguir innovando en el &aacute;mbito de la digitalizaci&oacute;n, y de empresas de expositores met&aacute;licos</p></div></article><article class="article02"><header><div class="title03">DIARI DE GIRONA - INFORMACION GENERAL</div><ul class="meta"><li>Publicada el 23/01/2018</ul></header><div><p>NOA BRANDS EUROPE, una empresa de desarrollo de productos de merchandising, ha decidido trasladar su sede y dejar La Garrotxa. La compa&ntilde;&iacute;a, que emplea a 210 personas y cuya facturaci&oacute;n supera los 25 millones de euros, deja Olot y se traslada a la calle Marqu&eacute;s de Cubas, de Madrid.</p></div></article><article class="article02"><header><div class="title03">EXPANSI&Oacute;N DE CATALUNA - INFORMACION FINANCIERA</div><ul class="meta"><li>Publicada el 16/10/2015</ul></header><div><p>Atrezzo celebra sus 25 a&ntilde;os con un volumen de negocio de 20 millones y 300 empleados en plantilla. Atrezzo empez&oacute; en Olot, donde a&uacute;n mantiene su f&aacute;brica y su centro log&iacute;stico con 250 empleados, y ha ido creciendo hasta llegar a vender en m&aacute;s de diez pa&iacute;ses de Europa, Estados Unidos y Asia, con sede en Barcelona y f&aacute;bricas en M&eacute;xico, EEUU y China. La previsi&oacute;n para este a&ntilde;o es aumentar un 15% su volumen de negocio y un 12% su plantilla. Ahora el foco est&aacute; en Estados Unidos y Latinoam&eacute;rica. As&iacute;, quiere crecer y abrir nuevos mercados, tal y como afirma Marisa Tendero, directora ejecutiva de NOABRANDS, el grupo al que pertenece Atrezzo, que invierte entre 2 y 3 millones al a&ntilde;o en la empresa.</p></div></article><article class="article02"><header><div class="title03">LA VANGUARDIA - INFORMACION PARTICIPACIONES</div><ul class="meta"><li>Publicada el 18/03/2015</ul></header><div><p>THE VISUALITY CORPORATION, que dirige Marisa Tendero, ha firmado la adquisici&oacute;n de Falcon Industries, empresa de Nueva Jersey especializada en la fabricaci&oacute;n de expositores y estanter&iacute;as met&aacute;licas. THE VISUALITY Group tiene sus centros de producci&oacute;n en M&eacute;xico, Olot, China y Denver. Con unas ventas de 70 millones de euros en 2014 y una plantilla de 700 personas, incorpora ahora un negocio de 15 millones de euros de Falcon.</p></div></article><article class="article02"><header><div class="title03">EXPANSI&Oacute;N - INFORMACION PARTICIPACIONES</div><ul class="meta"><li>Publicada el 29/09/2014</ul></header><div><p>La firma de capital riesgo MIURA ha comprado THE VISUALITY CORPORATION (TVC), una compa&ntilde;&iacute;a con sede en Barcelona especializada en merchandising para tiendas y locales de la gran distribuci&oacute;n. La adquisici&oacute;n se ha articulado a trav&eacute;s de la creaci&oacute;n de un veh&iacute;culo de inversi&oacute;n en el que participa MIURA y Porterhouse, un family office europeo, que en conjunto controlan el 90% del capital. Esta es la primera inversi&oacute;n de MIURA con su nuevo fondo, aunque ya fue accionista de NOA VISUAL GROUP, germen de la actual TVC. Porterhouse compr&oacute; en 2013 la espa&ntilde;ola NOA, firma que fusion&oacute; con un competidor estadounidense, que estaba en manos de Blue Sage, para crear THE VISUAL CORPORATION.</p></div></article></div><br><div class="row mobile-hidden"><div class="col col03"><section><p class="title02">categor&iacute;as</p><ul class="list02 list02b"><li class="first-child"><a href="http://empresite.eleconomista.es/Actividad/HOGAR/" title="Ir a hogar">hogar</a><li><a href="http://empresite.eleconomista.es/Actividad/DISENO/" title="Ir a Dise&ntilde;o">Dise&ntilde;o</a><li><a href="http://empresite.eleconomista.es/Actividad/INTERIORISMO/" title="Ir a interiorismo">interiorismo</a><li><a href="http://empresite.eleconomista.es/Actividad/TIENDAS/" title="Ir a tiendas">tiendas</a><li><a href="http://empresite.eleconomista.es/Actividad/PUBLICIDAD/" title="Ir a Publicidad">Publicidad</a><li><a href="http://empresite.eleconomista.es/Actividad/BARCELONA/" title="Ir a Barcelona">Barcelona</a><li><a href="http://empresite.eleconomista.es/Actividad/ACCESSORIES/" title="Ir a accessories">accessories</a><li><a href="http://empresite.eleconomista.es/Actividad/FIXTURES/" title="Ir a fixtures">fixtures</a><li><a href="http://empresite.eleconomista.es/Actividad/DISPLAYS/" title="Ir a displays">displays</a><li><a href="http://empresite.eleconomista.es/Actividad/DECORACION/" title="Ir a Decoraci&Ucirc;n">Decoraci&Ucirc;n</a><li><a href="http://empresite.eleconomista.es/Actividad/RETAIL/" title="Ir a Retail">Retail</a><li><a href="http://empresite.eleconomista.es/Actividad/MANNEQUINS/" title="Ir a mannequins">mannequins</a><li><a href="http://empresite.eleconomista.es/Actividad/MANIQUIS/" title="Ir a maniquis">maniquis</a><li><a href="http://empresite.eleconomista.es/Actividad/VISUAL-MERCHANDISING/" title="Ir a Visual merchandising">Visual merchandising</a><li><a href="http://empresite.eleconomista.es/Actividad/MANIQUI/" title="Ir a maniqui">maniqui</a><li><a href="http://empresite.eleconomista.es/Actividad/TORSOS/" title="Ir a torsos">torsos</a><li><a href="http://empresite.eleconomista.es/Actividad/WINDOW-DRESSING/" title="Ir a window dressing">window dressing</a><li><a href="http://empresite.eleconomista.es/Actividad/MANNEQUIN/" title="Ir a mannequin">mannequin</a><li><a href="http://empresite.eleconomista.es/Actividad/OBJETO-REGALO/" title="Ir a Objeto de regalo">Objeto de regalo</a><li><a href="http://empresite.eleconomista.es/Actividad/ATREZZO-MANIQUIES/" title="Ir a Atrezzo maniquies">Atrezzo maniquies</a><li><a href="http://empresite.eleconomista.es/Actividad/DISPLAY-MANNEQUINS/" title="Ir a display mannequins">display mannequins</a><li><a href="http://empresite.eleconomista.es/Actividad/WINDOW-DISPLAY/" title="Ir a window display">window display</a><li><a href="http://empresite.eleconomista.es/Actividad/DISPLAY-ACCESSORIES/" title="Ir a display accessories">display accessories</a><li><a href="http://empresite.eleconomista.es/Actividad/DISPLAY-ACCESSORY/" title="Ir a display accessory">display accessory</a><li><a href="http://empresite.eleconomista.es/Actividad/D-MANNEQUIN/" title="Ir a d mannequin">d mannequin</a><li><a href="http://empresite.eleconomista.es/Actividad/DESIGNER-MANNEQUINS/" title="Ir a designer mannequins">designer mannequins</a><li><a href="http://empresite.eleconomista.es/Actividad/MANNEQUIN-WORLD/" title="Ir a mannequin world">mannequin world</a><li><a href="http://empresite.eleconomista.es/Actividad/BUSTS/" title="Ir a busts">busts</a><li><a href="http://empresite.eleconomista.es/Actividad/WOMAN-MANNEQUINS/" title="Ir a woman mannequins">woman mannequins</a><li><a href="http://empresite.eleconomista.es/Actividad/ABSTRACT-MANNEQUINS/" title="Ir a abstract mannequins">abstract mannequins</a><li><a href="http://empresite.eleconomista.es/Actividad/MEN-MANNEQUINS/" title="Ir a men mannequins">men mannequins</a><li><a href="http://empresite.eleconomista.es/Actividad/CHILDREN-MANNEQUIN/" title="Ir a children mannequin">children mannequin</a><li class="last-child"><a href="http://empresite.eleconomista.es/Actividad/FASHION-WINDOWS/" title="Ir a fashion windows">fashion windows</a></ul></section></div></div><br><div class="solodesktop" id="banner-certificado-gestion" style="text-align:center;"></div><script type="text/javascript">
